{
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-unused-vars": "warn",
    "radix": "warn",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "crlf"
      }
    ]
  }
}
